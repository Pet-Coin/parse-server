{
  "name": "parse-server-example",
  "version": "1.4.0",
  "description": "An example Parse API server using the parse-server module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParsePlatform/parse-server-example"
  },
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "coverage": "nyc jasmine",
    "lint": "eslint --cache ./cloud && eslint --cache index.ts && eslint --cache ./spec",
    "lint-fix": "eslint --cache --fix ./cloud && eslint --cache --fix index.ts && eslint --cache --fix ./spec",
    "prettier": "prettier --write '{cloud,spec}/{**/*,*}.ts' 'index.ts'",
    "start": "node index.ts",
    "dev": "nodemon",
    "test": "mongodb-runner start && jasmine"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "envalid": "^7.3.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "4.18.1",
    "moralis": "^2.2.0",
    "node-fetch": "^3.2.10",
    "parse": "3.4.2",
    "parse-dashboard": "^4.1.4",
    "parse-server": "^5.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "8.15.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jasmine": "4.1.0",
    "mongodb-runner": "4.9.0",
    "nodemon": "^2.0.19",
    "nyc": "15.1.0",
    "prettier": "2.6.2"
  },
  "engines": {
    "node": ">=12.22.10 <19"
  }
}
